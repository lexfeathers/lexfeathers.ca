{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@62c5e0cdf6d61235805fb21979c1c83cb00f9a45/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@8091117168e698cc6d83a98b4e87d9e0bcb62852/"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
